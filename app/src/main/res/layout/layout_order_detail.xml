<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content">

    <ImageView
            android:layout_width="@dimen/dimen_56_dp"
            android:layout_height="@dimen/dimen_56_dp" app:srcCompat="@drawable/profile_img"
            android:id="@+id/imageView11"
            app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline20"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline19" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline20" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="horizontal"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView71" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline20"
            app:layout_constraintStart_toEndOf="@+id/imageView11" android:layout_marginStart="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView72" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView71"
            app:layout_constraintStart_toEndOf="@+id/imageView11" android:layout_marginStart="8dp"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline24" app:layout_constraintGuide_begin="230dp"
                                                android:orientation="horizontal"/>
    <include android:layout_width="0dp" android:layout_height="0dp"
             layout="@layout/layout_order_details"
             app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginStart="8dp"
             android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/imageView11"
             app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
             app:layout_constraintBottom_toTopOf="@+id/guideline24"/>
</androidx.constraintlayout.widget.ConstraintLayout>
